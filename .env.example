# PromptTracker Configuration
# Set to 'true' to use real LLM API calls, 'false' for mock responses
PROMPT_TRACKER_USE_REAL_LLM=false

# LLM Provider API Keys
# OpenAI (for GPT-4, GPT-3.5, etc.)
# Get your API key from https://platform.openai.com/api-keys
OPENAI_API_KEY=your_openai_api_key_here

# Anthropic (for Claude models)
# Get your API key from https://console.anthropic.com/
# ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Google (for Gemini models)
# Get your API key from https://makersuite.google.com/app/apikey
# GOOGLE_API_KEY=your_google_api_key_here

# Cohere
# Get your API key from https://dashboard.cohere.com/api-keys
# COHERE_API_KEY=your_cohere_api_key_here
